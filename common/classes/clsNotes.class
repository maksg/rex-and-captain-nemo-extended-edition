OBJECT=I1
I1:TYPE=INTEGER

OBJECT=SNOTESSTRING
SNOTESSTRING:TYPE=STRING

OBJECT=INOTESSTRONAL
INOTESSTRONAL:TYPE=INTEGER
INOTESSTRONAL:VALUE=1

OBJECT=ANNCLOSE
ANNCLOSE:TYPE=ANIMO
ANNCLOSE:VISIBLE=TRUE
ANNCLOSE:FILENAME=$COMMON\NOTES_CLOSE.ANN
ANNCLOSE:TOCANVAS=TRUE
ANNCLOSE:PRIORITY=22010
ANNCLOSE:FPS=16
ANNCLOSE:PRELOAD=TRUE
ANNCLOSE:RELEASE=TRUE
ANNCLOSE:MONITORCOLLISION=FALSE
ANNCLOSE:MONITORCOLLISIONALPHA=FALSE

OBJECT=ANNLEFT
ANNLEFT:TYPE=ANIMO
ANNLEFT:VISIBLE=TRUE
ANNLEFT:FILENAME=$COMMON\NOTES_LEFT.ANN
ANNLEFT:TOCANVAS=TRUE
ANNLEFT:PRIORITY=22010
ANNLEFT:FPS=16
ANNLEFT:PRELOAD=TRUE
ANNLEFT:RELEASE=TRUE
ANNLEFT:MONITORCOLLISION=FALSE
ANNLEFT:MONITORCOLLISIONALPHA=FALSE

OBJECT=ANNRIGHT
ANNRIGHT:TYPE=ANIMO
ANNRIGHT:VISIBLE=TRUE
ANNRIGHT:FILENAME=$COMMON\NOTES_RIGHT.ANN
ANNRIGHT:TOCANVAS=TRUE
ANNRIGHT:PRIORITY=22010
ANNRIGHT:FPS=16
ANNRIGHT:PRELOAD=TRUE
ANNRIGHT:RELEASE=TRUE
ANNRIGHT:MONITORCOLLISION=FALSE
ANNRIGHT:MONITORCOLLISIONALPHA=FALSE

OBJECT=ANNSTRONAN
ANNSTRONAN:TYPE=ANIMO
ANNSTRONAN:VISIBLE=FALSE
ANNSTRONAN:FILENAME=$COMMON\NOTES_STRONY_N.ANN
ANNSTRONAN:TOCANVAS=TRUE
ANNSTRONAN:PRIORITY=22010
ANNSTRONAN:FPS=16
ANNSTRONAN:PRELOAD=TRUE
ANNSTRONAN:RELEASE=TRUE
ANNSTRONAN:MONITORCOLLISION=FALSE
ANNSTRONAN:MONITORCOLLISIONALPHA=FALSE

OBJECT=ANNSTRONAP
ANNSTRONAP:TYPE=ANIMO
ANNSTRONAP:VISIBLE=FALSE
ANNSTRONAP:FILENAME=$COMMON\NOTES_STRONY_P.ANN
ANNSTRONAP:TOCANVAS=TRUE
ANNSTRONAP:PRIORITY=22010
ANNSTRONAP:FPS=16
ANNSTRONAP:PRELOAD=TRUE
ANNSTRONAP:RELEASE=TRUE
ANNSTRONAP:MONITORCOLLISION=FALSE
ANNSTRONAP:MONITORCOLLISIONALPHA=FALSE

OBJECT=IMGNOTESBKG
IMGNOTESBKG:TYPE=IMAGE
IMGNOTESBKG:VISIBLE=TRUE
IMGNOTESBKG:FILENAME=$COMMON\NOTES_BKG.IMG
IMGNOTESBKG:TOCANVAS=TRUE
IMGNOTESBKG:PRIORITY=22000
IMGNOTESBKG:PRELOAD=TRUE
IMGNOTESBKG:RELEASE=TRUE
IMGNOTESBKG:MONITORCOLLISION=FALSE
IMGNOTESBKG:MONITORCOLLISIONALPHA=FALSE

OBJECT=BUTNULL
BUTNULL:TYPE=BUTTON
BUTNULL:VISIBLE=TRUE
BUTNULL:RECT=0,0,800,600
BUTNULL:ENABLE=FALSE
BUTNULL:DRAGGABLE=FALSE
BUTNULL:GFXSTANDARD=IMGNOTESBKG

OBJECT=BTNCLOSE
BTNCLOSE:TYPE=BUTTON
BTNCLOSE:VISIBLE=TRUE
BTNCLOSE:ENABLE=FALSE
BTNCLOSE:DRAGGABLE=FALSE
BTNCLOSE:GFXSTANDARD=ANNCLOSE
BTNCLOSE:ONCLICKED={ANNCLOSE^PREVFRAME();ZWIN^RUN();}
BTNCLOSE:ONFOCUSON={ANNCLOSE^NEXTFRAME();}
BTNCLOSE:ONFOCUSOFF={ANNCLOSE^PREVFRAME();}

OBJECT=BTNLEFT
BTNLEFT:TYPE=BUTTON
BTNLEFT:VISIBLE=TRUE
BTNLEFT:ENABLE=FALSE
BTNLEFT:DRAGGABLE=FALSE
BTNLEFT:GFXSTANDARD=ANNLEFT
BTNLEFT:ONCLICKED={@IF("INOTESSTRONAL",">","1","BFITMP146","");}
BTNLEFT:ONFOCUSON={ANNLEFT^NEXTFRAME();}
BTNLEFT:ONFOCUSOFF={ANNLEFT^PREVFRAME();}

OBJECT=BTNRIGHT
BTNRIGHT:TYPE=BUTTON
BTNRIGHT:VISIBLE=TRUE
BTNRIGHT:ENABLE=FALSE
BTNRIGHT:DRAGGABLE=FALSE
BTNRIGHT:GFXSTANDARD=ANNRIGHT
BTNRIGHT:ONCLICKED={@IF("INOTESSTRONAL","<","33","BFITMP147","");}
BTNRIGHT:ONFOCUSON={ANNRIGHT^NEXTFRAME();}
BTNRIGHT:ONFOCUSOFF={ANNRIGHT^PREVFRAME();}

OBJECT=SFX_NOTES
SFX_NOTES:TYPE=SOUND
SFX_NOTES:FILENAME=SFX_NOTES.WAV
SFX_NOTES:PRELOAD=FALSE
SFX_NOTES:FLUSHAFTERPLAYED=FALSE

OBJECT=CONSTRUCTOR
CONSTRUCTOR:TYPE=BEHAVIOUR
CONSTRUCTOR:CODE={BUTNULL^SETPRIORITY(22000);LOAD^RUN();}


OBJECT=LOAD
LOAD:TYPE=BEHAVIOUR
LOAD:CODE={SNOTESSTRING^SET(GSAVE^GET("NOTES"));INOTESSTRONAL^SET(GSAVE^GET("NOTES_STRONA"));}


OBJECT=SET
SET:TYPE=BEHAVIOUR
SET:CODE={@INT("INOTESSTRONANO",$1);@STRING("SNOTESSTRONASTAN",$2);@IF(SNOTESSTRING^GET(INOTESSTRONANO),"!_","SNOTESSTRONASTAN","BFITMP149","BFITMP150");}


OBJECT=GET
GET:TYPE=BEHAVIOUR
GET:CODE={@INT("INOTESSTRONANO",$1);@STRING("SNOTESSTRONASTAN",SNOTESSTRING^GET(INOTESSTRONANO));@RETURN(SNOTESSTRONASTAN);}


OBJECT=ROZWIN
ROZWIN:TYPE=BEHAVIOUR
ROZWIN:CODE={BUTNULL^ENABLE();BTNCLOSE^ENABLE();BTNLEFT^ENABLE();BTNRIGHT^ENABLE();ANNSTRONAN^SHOW();ANNSTRONAP^SHOW();BGMOUSEONMOVEENABLED^SET(FALSE);BEHSETSTRONY^RUN();}


OBJECT=ZWIN
ZWIN:TYPE=BEHAVIOUR
ZWIN:CODE={BUTNULL^DISABLE();BTNCLOSE^DISABLE();BTNLEFT^DISABLE();BTNRIGHT^DISABLE();ANNSTRONAN^HIDE();ANNSTRONAP^HIDE();BGMOUSEONMOVEENABLED^SET(TRUE);}


OBJECT=BEHSETSTRONY
BEHSETSTRONY:TYPE=BEHAVIOUR
BEHSETSTRONY:CODE={I1^SET(SNOTESSTRING^GET(INOTESSTRONAL,1));ANNSTRONAN^SETFRAME(["S"+INOTESSTRONAL],I1);I1^SET(SNOTESSTRING^GET([INOTESSTRONAL+1],1));ANNSTRONAP^SETFRAME(["S"+[INOTESSTRONAL+1]],I1);@IF("INOTESSTRONAL","_","1","BFITMP151","BFITMP152");@IF("INOTESSTRONAL","_","33","BFITMP153","BFITMP154");}


OBJECT=DISABLECLOSE
DISABLECLOSE:TYPE=BEHAVIOUR
DISABLECLOSE:CODE={BTNCLOSE^DISABLE();}


OBJECT=GETSITE
GETSITE:TYPE=BEHAVIOUR
GETSITE:CODE={@RETURN(INOTESSTRONAL);}


OBJECT=BFITMP146
BFITMP146:TYPE=BEHAVIOUR
BFITMP146:CODE={INOTESSTRONAL^SUB(2);BEHSETSTRONY^RUN();GSAVE^SET("NOTES_STRONA",INOTESSTRONAL);SFX_NOTES^PLAY();}


OBJECT=BFITMP147
BFITMP147:TYPE=BEHAVIOUR
BFITMP147:CODE={INOTESSTRONAL^ADD(2);BEHSETSTRONY^RUN();GSAVE^SET("NOTES_STRONA",INOTESSTRONAL);SFX_NOTES^PLAY();}


OBJECT=BFITMP148
BFITMP148:TYPE=BEHAVIOUR
BFITMP148:CODE={INOTESSTRONAL^DEC();}


OBJECT=BFITMP149
BFITMP149:TYPE=BEHAVIOUR
BFITMP149:CODE={INOTESSTRONAL^SET(INOTESSTRONANO);@IF([INOTESSTRONAL%2],"_","0","BFITMP148","");SNOTESSTRING^REPLACEAT(INOTESSTRONANO,SNOTESSTRONASTAN);GSAVE^SET("NOTES",SNOTESSTRING);GSAVE^SET("NOTES_STRONA",INOTESSTRONAL);@RETURN("DONE");}


OBJECT=BFITMP150
BFITMP150:TYPE=BEHAVIOUR
BFITMP150:CODE={@RETURN("ERR_ALR");}


OBJECT=BFITMP151
BFITMP151:TYPE=BEHAVIOUR
BFITMP151:CODE={BTNLEFT^DISABLE();}


OBJECT=BFITMP152
BFITMP152:TYPE=BEHAVIOUR
BFITMP152:CODE={BTNLEFT^ENABLE();}


OBJECT=BFITMP153
BFITMP153:TYPE=BEHAVIOUR
BFITMP153:CODE={BTNRIGHT^DISABLE();}


OBJECT=BFITMP154
BFITMP154:TYPE=BEHAVIOUR
BFITMP154:CODE={BTNRIGHT^ENABLE();}


